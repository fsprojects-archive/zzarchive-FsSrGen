    },
    "frameworks": {
        "net46": {
            "dependencies": {   
                "FSharp.Core": "4.0.0.1"
            }
        },
        "netcoreapp1.0": { 
            "dependencies": {
                "Microsoft.FSharp.Core.netcore": "1.0.0-alpha-*",
                "Microsoft.NetCore.App":{
                    "type":"platform",
                    "version":"1.0.1"    
                }
            }
        ,   "imports": "dnxcore50"
        }
    }
