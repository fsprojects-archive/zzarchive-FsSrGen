{
    "version": "1.0.0",
    "buildOptions": {
        "emitEntryPoint": true,
        "compilerName": "fsc",
        "compile": {
            "includeFiles": [
                "FSComp.fs",
                "Program.fs"
            ]
        },
        "embed": {
            "includeFiles": [
                "FSComp.resx"
            ]
        }
    },
    "tools": {
        "dotnet-compile-fsc": {
            "version": "1.0.0-preview2-*",
            "imports": "dnxcore50"
        }
