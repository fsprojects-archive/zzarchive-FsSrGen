{
    "version": "3.0.1",
    "buildOptions": {
        "emitEntryPoint": true,
        "compilerName": "fsc",
        "additionalArguments": [ "--standalone" ],
        "compile": {
            "includeFiles": [
                "Program.fs"
            ]
        }
    },
    "tools": {
        "dotnet-compile-fsc": {
            "version": "1.0.0-preview2-*",
            "imports": "dnxcore50"
        }
    },
    "frameworks": {
        "net46" : {
            "dependencies": {
                "FSharp.Core": "4.0.0.1"
            },
            "frameworkAssemblies": {
                "System.Xml": ""
            }
        }       
    }
}
